{
  "name": "multisig-alert",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "db:generate-schemas": "node scripts/switch-db.js",
    "db:use-sqlite": "cp prisma/schema.sqlite prisma/schema.prisma && prisma generate",
    "db:use-postgres": "cp prisma/schema.postgres prisma/schema.prisma && prisma generate",
    "db:local": "npm run db:use-sqlite",
    "db:production": "npm run db:use-postgres"
  },
  "dependencies": {
    "@hugeicons/react": "^1.0.5",
    "@prisma/client": "^6.14.0",
    "@safe-global/api-kit": "^4.0.0",
    "@safe-global/safe-core-sdk-types": "^5.1.0",
    "@upstash/redis": "^1.35.3",
    "@vercel/og": "^0.8.5",
    "next": "15.5.0",
    "prisma": "^6.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-toastify": "^11.0.5",
    "thirdweb": "^5.105.37"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.5.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-toastify": "^4.0.2",
    "daisyui": "^5.0.50",
    "eslint": "^9",
    "eslint-config-next": "^15.5.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
